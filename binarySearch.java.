class Solution {
    public int binarysearch(int[] nums, int x) {
        int high=nums.length-1,low=0;
        int result=-1;
        while(low<=high)
        {
            int mid=low+(high-low)/2;
            if(nums[mid]==x)
            {
                result=mid;
                high=mid-1;
            }
            else if(x<nums[mid]) high=mid-1;
            else low=mid+1;
        }
        return result;
        
    }
}