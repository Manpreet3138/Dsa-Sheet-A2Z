class Solution {
    public List<List<Integer>> generate(int n) {
        List<List<Integer>> list=new ArrayList<>();
		for(int i=0;i<n;i++)
		{
			list.add(new ArrayList<>());
			int number=1;
			for(int j=0;j<=i;j++)
			{
				list.get(i).add(number);
				number=number*(i-j)/(j+1);
			}
		}
		return list;
    }
}
