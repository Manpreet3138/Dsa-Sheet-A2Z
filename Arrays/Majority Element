class Solution {
    public int majorityElement(int[] arr) {
        int n=arr.length;
        int ele=0;
        int count=0;
        for(int i=0;i<n;i++)
        {
            if(count==0)
            {
                ele=arr[i];
            }
            count += (arr[i]==ele) ? 1 : -1;
        }
        return ele;
    }
}
